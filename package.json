{
  "name": "gatsby-starter-bootstrap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gatsby": "^2.24.87",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-google-fonts-v2": "^0.0.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hotkeys-hook": "^2.3.1",
    "tailwindcss": "1.9.6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.3",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-catch-links": "^2.3.15",
    "gatsby-plugin-feed": "^2.5.19",
    "gatsby-plugin-google-analytics": "^2.3.19",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-graphql-config": "^0.0.3",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-netlify": "^2.3.24",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sass": "^2.3.21",
    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-source-filesystem": "^2.3.36",
    "gatsby-transformer-sharp": "^2.5.20",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "10.5.0",
    "lodash": "4.17.20",
    "node-sass": "4.14.1",
    "prettier": "^2.1.2",
    "tailwindcss-export-config": "^2.3.0",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ]
  },
  "scripts": {
    "start": "yarn dev",
    "prebuild": "yarn tailwind:export",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "tailwind:export": "tailwindcss-export-config --config=./tailwind.config.js --destination=./src/assets/style/_tailwindvars  --format=scss --quoted-keys=true && prettier src/assets/style/_tailwindvars.scss -w",
    "predev": "yarn tailwind:export",
    "dev": "ENABLE_GATSBY_REFRESH_ENDPOINT=1 gatsby develop",
    "lint": "eslint src/**/* --fix --ext .ts,.tsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "gatsby clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}

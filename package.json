    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-hotkeys-hook": "^3.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",

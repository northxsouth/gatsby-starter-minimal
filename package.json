  "private": true,
  "resolutions": {
    "graphql": "14.7.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "^2.30.1",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-google-fonts-v2": "^0.0.6",
    "lodash": "4.17.20",

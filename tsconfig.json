{
  "compilerOptions": {
    "target": "ES2015",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "resolveJsonModule": true
  },
  "include": ["./src/**/*", "./config/**/*", "./gatsby*.ts"]
}
